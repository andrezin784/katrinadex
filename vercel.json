{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd app && npm install && npm run build",
  "outputDirectory": "app/.next",
  "rewrites": [
    { "source": "/(.*)", "destination": "/app/$1" }
  ],
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "silent": true
  }
}








